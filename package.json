{
  "name": "@michijs/storybook-utils",
  "license": "MIT",
  "version": "0.0.1",
  "description": "Utilities for the Michijs storybook template",
  "files": [
    "dist/"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "scripts": {
    "dist": "tsc",
    "link": "tsc && npm link --force && tsc -w"
  },
  "keywords": [
    "web-components"
  ],
  "devDependencies": {
    "lint-staged": "13.1.1",
    "rome": "11.0.0",
    "simple-git-hooks": "2.8.1",
    "typescript": "5.0.2"
  },
  "dependencies": {
    "@storybook/addon-toolbars": "6.5.16",
    "@storybook/addons": "6.5.16",
    "@storybook/theming": "6.5.16",
    "@storybook/types": "7.0.0-alpha.44",
    "@michijs/michijs": "1.0.4"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "rome check --apply-suggested"
  }
}
