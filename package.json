{
  "name": "@michijs/storybook-utils",
  "license": "MIT",
  "version": "7.0.5",
  "description": "Utilities for the Michijs Storybook template",
  "files": [
    "dist/"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "scripts": {
    "dist": "tsc",
    "publish-npm": "npm run dist && npm publish --access public",
    "link": "tsc && npm link --force && tsc -w"
  },
  "keywords": [
    "web-components"
  ],
  "devDependencies": {
    "typescript": "5.2.2"
  },
  "peerDependencies": {
    "@storybook/theming": "7.0.5",
    "@storybook/types": "7.0.5",
    "@michijs/michijs": "1.1.7"
  }
}
